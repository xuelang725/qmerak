		<servlet-class>net.qmerak.webmsg.server.ContinuationServlet
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>net.qmerak.webmsg.server.ContinuationServlet
		</servlet-name>
		<url-pattern>/continue</url-pattern>
	</servlet-mapping>

</web-app>